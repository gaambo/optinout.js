{
  "name": "optinout.js",
  "version": "1.1.2",
  "description": "a simple js framework for handling opt-in and opt-out for tracking & cookies",
  "keywords": [
    "optinout",
    "optin",
    "optout",
    "privacy",
    "gdpr",
    "optout tracking"
  ],
  "main": "dist/optinout.js",
  "jsnext:main": "src/index.js",
  "commonModule": "dist/optinout.cjs.js",
  "module": "dist/optinout.esm.js",
  "browser": "dist/optinout.umd.js",
  "scripts": {
    "build": "gulp build",
    "lint": "eslint src",
    "test": "npm run unit-test",
    "unit-test": "jest",
    "browser-test": "testcafe 'chrome' test/browser/",
    "test-server": "node test/server.js",
    "example-server": "node examples/server.js",
    "postversion": "git push --follow-tags"
  },
  "repository": "gitlab:optinout/optinout.js",
  "author": "fabian todt",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/optinout/optinout.js/issues"
  },
  "homepage": "https://gitlab.com/optinout/optinout.js#README",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.13.0",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-rollup": "^2.16.2",
    "gulp-uglify": "^3.0.1",
    "http-server": "^0.11.1",
    "jest": "^22.4.4",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.52.3",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-stream": "^1.24.1",
    "testcafe": "^0.19.2",
    "uglify-es": "^3.3.10",
    "vinyl-source-stream": "^2.0.0"
  },
  "files": [
    "src",
    "dist",
    "examples",
    "DOCUMENTATION.md"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json",
      "html"
    ],
    "testURL": "http://localhost"
  }
}
